{
    "name": "use-typed-reducer",
    "type": "module",
    "version": "4.3.0",
    "description": "use-reducer React hook alternative",
    "author": "g4rcez",
    "license": "MIT",
    "source": "./src/index.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "cypress": "cypress open",
        "dev": "rollup -c --watch --bundleConfigAsCjs",
        "format": "prettier --write \"{example,src,tests}/**/*.{js,jsx,ts,tsx}\"",
        "prepare": "pnpm build",
        "test:unit": "vitest run",
        "build": "rm -rf ./dist; rollup -c --bundleConfigAsCjs; rm dist/tsconfig.tsbuildinfo",
        "test": "NODE_ENV=test vitest run"
    },
    "dependencies": {
        "use-sync-external-store": "1.6.0"
    },
    "peerDependencies": {
        "react": ">= 16.8.3"
    },
    "devDependencies": {
        "@rollup/plugin-babel": "6.1.0",
        "@rollup/plugin-commonjs": "29.0.0",
        "@rollup/plugin-node-resolve": "16.0.3",
        "@rollup/plugin-strip": "3.0.4",
        "@rollup/plugin-typescript": "12.3.0",
        "@rollup/plugin-url": "8.0.2",
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/react": "16.3.2",
        "@types/node": "25.2.3",
        "@types/react": "19.2.14",
        "@types/react-dom": "19.2.3",
        "@types/use-sync-external-store": "1.5.0",
        "@vitejs/plugin-react": "5.1.4",
        "jsdom": "28.1.0",
        "prettier": "3.8.1",
        "react": "19.2.4",
        "react-dom": "19.2.4",
        "rollup": "4.57.1",
        "rollup-plugin-peer-deps-external": "2.2.4",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "vite": "7.3.1",
        "vite-plugin-dts": "4.5.4",
        "vitest": "4.0.18"
    },
    "files": [
        "dist"
    ]
}
